[local-base]
name=Local CentOS-$releasever - Base
baseurl=http://<%="#{node['base']['master']}"%>/mrepo/centos$releasever-$basearch/RPMS.os/
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-CentOS-6
enabled=1

#released updates 
[local-updates]
name=Local CentOS-$releasever - Updates
baseurl=http://<%="#{node['base']['master']}"%>/mrepo/centos$releasever-$basearch/RPMS.updates/
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-CentOS-6
enabled=1

#contrib - packages by Centos Users
[local-misc]
name=Local CentOS-$releasever - Contrib
baseurl=http://<%="#{node['base']['master']}"%>/mrepo/lab-custom-x86_64/RPMS.misc/
gpgcheck=0
enabled=1
